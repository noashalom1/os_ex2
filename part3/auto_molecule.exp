#!/usr/bin/expect -f
set timeout 1
set host [lindex $argv 0]
set port [lindex $argv 1]

spawn ./molecule_requester $host $port

expect {
    "Connected to molecule server" {}
    timeout { puts "âŒ Failed to connect to molecule server"; exit 1 }
}

set f [open "commands_molecular.txt"]
while {[gets $f line] >= 0} {
    send -- "$line\r"
    expect {
        -re "Server response:.*\n" {}
        timeout {}
    }
    sleep 0.1
}
close $f

sleep 1
send -- "\004"  ;# 
expect eof
